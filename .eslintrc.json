{
	"env": { "browser": true },
	"extends": ["airbnb", "prettier"],
	"plugins": ["prettier"],
	"parser": "babel-eslint",
	"rules": {
		"prettier/prettier": ["error"]
	},
	"overrides": [
		{
			"files": ["*.test.js"],
			"rules": {
				"no-unused-expressions": "off",
				"no-undef": "off"
			}
		}
	]
}
